<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Document</title>
</head>
<body>
  <h1><a href="/presenters">Presenters</a></h1>
  <h3><a href="/admin">Back</a></h3>
  <h3><a href="/presenters/new">Create New</a></h3>
  <hr>
  <% presenters.forEach(function(presenter) { %>
    <% Object.keys(presenter).forEach(function(key) { %>
      <p><%= key %>: <%= presenter[key] %></p>
    <% }) %>
    <a href="/presenters/<%= presenter.id %>"><button>View</button></a>
    <a href="/presenters/<%= presenter.id %>/edit"><button>Edit</button></a>
    <form action="/presenters/<%= presenter.id %>" method="post">
      <input type="hidden" name="_method" value="delete">
      <button>Delete</button>
    </form>
    <hr>
  <% }) %>
</body>
</html>